{
  "name": "dmd-readme-api",
  "version": "1.0.0-beta.5",
  "description": "A jsdoc2md template plugin generating elegant, compact one-page APIs in markdown, suitable for embedding in a README.md.",
  "scripts": {
    "build": "",
    "test": "make test",
    "prepack": "make build",
    "preversion": "make qa",
    "qa": "make qa"
  },
  "keywords": [
    "api",
    "dmd",
    "documentaiton",
    "jsdoc",
    "jsdoc2md",
    "markdown",
    "md",
    "readme"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "Apache 2.0",
  "repository": "git+ssh://git@github.com/liquid-labs/dmd-readme-api.git",
  "bugs": {
    "url": "https://github.com/liquid-labs/dmd-readme-api/issues"
  },
  "homepage": "https://github.com/liquid-labs/dmd-readme-api#readme",
  "_comply": {
    "orgKey": "@liquid-labs"
  },
  "devDependencies": {
    "@liquid-labs/dmd": "^6.3.1",
    "@liquid-labs/sdlc-resource-jest": "^1.0.0-alpha.6",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0"
  },
  "_sdlc": {
    "jestConfig": {
      "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/index.[mc]?jsx?",
        "/test/",
        "\\.test\\.[^.]+\\.[mc]?jsx?",
        "<rootDir>/test-staging/",
        "<rootDir>/dist/",
        "<rootDir>/qa/",
        "<rootDir>/coverage/",
        ".eslintrc.js"
      ]
    }
  },
  "dependencies": {
    "extract-topic": "^1.0.0-beta.6"
  }
}
